---5th script

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Function to create the Screen GUI
local function createGui()
    -- Create the Screen GUI
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "GetUpGui"
    screenGui.ResetOnSpawn = false -- Prevent GUI from disappearing on reset
    screenGui.Parent = playerGui

    -- Get Up Button
    local getUpButton = Instance.new("TextButton")
    getUpButton.Size = UDim2.new(0, 100, 0, 50)
    getUpButton.Position = UDim2.new(0, 120, 1, -280) -- Positioned next to the Lay Down button
    getUpButton.Text = "Get Up"
    getUpButton.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text for contrast
    getUpButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)  -- Dark background for edgy feel
    getUpButton.Font = Enum.Font.GothamBold  -- Bold and modern font
    getUpButton.TextSize = 20  -- Slightly smaller text size for consistency
    getUpButton.TextStrokeTransparency = 0.7  -- Slight text stroke for sharp contrast
    getUpButton.BorderSizePixel = 0  -- No border to keep the sleek look
    getUpButton.BackgroundTransparency = 0.3  -- Slight transparency for depth
    getUpButton.Parent = screenGui

    -- Add a UI corner for rounded edges
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)  -- Rounded corners with radius of 12
    corner.Parent = getUpButton

    -- Add hover effect
    getUpButton.MouseEnter:Connect(function()
        getUpButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)  -- Darker gray on hover for effect
        getUpButton.TextColor3 = Color3.fromRGB(255, 200, 0)  -- Goldish text color on hover for an edgy pop
    end)

    getUpButton.MouseLeave:Connect(function()
        getUpButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)  -- Return to original dark color
        getUpButton.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text when not hovered
    end)

    -- Function to make the character jump and play the animation
    local function getUp()
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        local animator = humanoid:FindFirstChildOfClass("Animator")

        if humanoid and animator then
            -- Stop all currently playing animations
            for _, track in ipairs(animator:GetPlayingAnimationTracks()) do
                track:Stop()
            end

            -- Play the new animation
            local animation = Instance.new("Animation")
            animation.AnimationId = "rbxassetid://15693621070"
            local animationTrack = animator:LoadAnimation(animation)

            animationTrack:Play() -- Play the animation
            animationTrack.TimePosition = 1.60 -- Start from 1.60 seconds
            animationTrack:AdjustSpeed(-1) -- Play the animation in reverse

            humanoid:ChangeState(Enum.HumanoidStateType.Jumping) -- Make the character jump

            wait(1) -- Wait for 1 second
            animationTrack:Stop() -- Stop the animation
        end
    end

    -- Button click event
    getUpButton.MouseButton1Click:Connect(getUp)
end

-- Create the GUI when the script runs
createGui()

-- Recreate the GUI after each reset
player.CharacterAdded:Connect(function()
    createGui()
end)

--6th script 

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Function to create the Screen GUI
local function createGui()
    -- Create the Screen GUI
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "ReverseGui"
    screenGui.Parent = playerGui

    -- Reverse Button
    local reverseButton = Instance.new("TextButton")
    reverseButton.Size = UDim2.new(0, 100, 0, 50)
    reverseButton.Position = UDim2.new(0, 10, 1, -210) -- Positioned at the bottom left
    reverseButton.Text = "Reverse" -- Initial text
    reverseButton.Font = Enum.Font.GothamBold
    reverseButton.TextSize = 20
    reverseButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35) -- Dark background for edgy feel
    reverseButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
    reverseButton.TextStrokeTransparency = 0.7 -- Slight text stroke for sharp contrast
    reverseButton.BorderSizePixel = 0 -- No border to keep the sleek look
    reverseButton.BackgroundTransparency = 0.3 -- Slight transparency for depth
    reverseButton.Parent = screenGui

    -- Add a UI corner for rounded edges
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12) -- Rounded corners with radius of 12
    corner.Parent = reverseButton

    -- Add hover effect
    reverseButton.MouseEnter:Connect(function()
        reverseButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60) -- Darker gray on hover for effect
        reverseButton.TextColor3 = Color3.fromRGB(255, 200, 0) -- Goldish text color on hover for an edgy pop
    end)

    reverseButton.MouseLeave:Connect(function()
        reverseButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35) -- Return to original dark color
        reverseButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text when not hovered
    end)

    local frames = {}
    local flashbackEnabled = false
    local flashbackLength = 60 -- seconds
    local flashbackSpeed = 1 -- frames to skip
    local RS = game:GetService("RunService")
    local LP = game.Players.LocalPlayer

    local function getCharacter()
        return LP.Character or LP.CharacterAdded:Wait()
    end

    local function getHRP(character)
        return character:FindFirstChild("HumanoidRootPart") or character.PrimaryPart
    end

    local function advanceMovement(char, hrp, hum)
        if #frames > flashbackLength * 500 then
            table.remove(frames, 1)
        end
        table.insert(frames, {
            hrp.CFrame,
            hrp.Velocity,
            hum:GetState(),
            hum.PlatformStand,
            char:FindFirstChildOfClass("Tool")
        })
    end

    local function revertMovement(char, hrp, hum)
        local num = #frames
        if num == 0 then
            return
        end

        for i = 1, flashbackSpeed do
            if num <= 0 then break end
            num = num - 1
        end

        local lastFrame = frames[num]
        table.remove(frames, num)

        hrp.CFrame = lastFrame[1]
        hrp.Velocity = -lastFrame[2]
        hum:ChangeState(lastFrame[3])
        hum.PlatformStand = lastFrame[4]

        local currentTool = char:FindFirstChildOfClass("Tool")
        if lastFrame[5] then
            if not currentTool then
                hum:EquipTool(lastFrame[5])
            end
        else
            hum:UnequipTools()
        end
    end

    reverseButton.MouseButton1Click:Connect(function()
        flashbackEnabled = not flashbackEnabled
        reverseButton.BackgroundColor3 = flashbackEnabled and Color3.fromRGB(255, 0, 0) or Color3.fromRGB(35, 35, 35) -- Toggle color
        reverseButton.Text = flashbackEnabled and "UnReverse" or "Reverse" -- Toggle text

        -- Adjust all currently playing animations
        local char = getCharacter()
        local hum = char:FindFirstChildWhichIsA("Humanoid")
        local animator = hum:FindFirstChildOfClass("Animator")

        if animator then
            for _, animationTrack in pairs(animator:GetPlayingAnimationTracks()) do
                animationTrack:AdjustSpeed(flashbackEnabled and -1 or 1) -- Set speed to -1 when reversing, reset to 1 otherwise
            end
        end
    end)

    RS.RenderStepped:Connect(function()
        local char = getCharacter()
        local hrp = getHRP(char)
        local hum = char:FindFirstChildWhichIsA("Humanoid")

        if flashbackEnabled then
            revertMovement(char, hrp, hum)
        else
            advanceMovement(char, hrp, hum)
        end
    end)
end

-- Create the GUI when the script runs
createGui()

-- Recreate the GUI after each reset
player.CharacterAdded:Connect(function()
    createGui()
end)


---7th script


local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Table to keep track of platforms created for players
local platforms = {}
local headSitActive = false -- Start with HeadSit inactive

-- Function to create and attach a platform above a player's head
local function createPlatformAboveHead(targetPlayer)
    local targetCharacter = targetPlayer.Character
    if targetCharacter then
        local head = targetCharacter:FindFirstChild("Head")
        if head then
            -- Check if a platform already exists for this player
            if not platforms[targetPlayer.UserId] then
                -- Create a new part for the platform
                local platform = Instance.new("Part")
                platform.Size = Vector3.new(3, 0.2, 3) -- Size of the platform
                platform.Anchored = true -- Keep it anchored
                platform.CanCollide = true -- Make it collidable
                platform.BrickColor = BrickColor.new("Bright blue") -- Change color as needed
                platform.Material = Enum.Material.SmoothPlastic
                platform.Parent = workspace

                -- Position the platform above the player's head (lowered)
                platform.CFrame = head.CFrame * CFrame.new(0, 0.8, 0) -- Adjusted height to be lower

                -- Store the platform in the table
                platforms[targetPlayer.UserId] = platform

                -- Update the platform's position on Heartbeat
                local connection
                connection = game:GetService("RunService").Heartbeat:Connect(function()
                    if targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
                        local updatedHead = targetPlayer.Character.Head
                        platform.CFrame = updatedHead.CFrame * CFrame.new(0, 0.8, 0) -- Update position above the head
                    else
                        -- Remove the platform if the player is no longer valid
                        platforms[targetPlayer.UserId] = nil
                        platform:Destroy()
                        connection:Disconnect() -- Disconnect the event
                    end
                end)
            end
        end
    end
end

-- Create the Screen GUI and Buttons
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "HeadSitGui"
screenGui.Parent = player:WaitForChild("PlayerGui")
screenGui.ResetOnSpawn = false -- Prevent GUI from disappearing on reset

-- HeadSit Button Design
local toggleButton = Instance.new("TextButton", screenGui)
toggleButton.Name = "HeadSitButton"
toggleButton.Size = UDim2.new(0, 100, 0, 50)  -- Keep the same size as before
toggleButton.Position = UDim2.new(0, 10, 1, -150)  -- Position at the top right of the screen
toggleButton.Text = "HeadSit: Off"
toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text for contrast
toggleButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)  -- Dark background for edgy feel
toggleButton.Font = Enum.Font.GothamBold  -- Bold and modern font
toggleButton.TextSize = 20  -- Slightly smaller text size for consistency
toggleButton.TextStrokeTransparency = 0.7  -- Slight text stroke for sharp contrast
toggleButton.BorderSizePixel = 0  -- No border to keep the sleek look
toggleButton.BackgroundTransparency = 0.3  -- Slight transparency for depth

-- Add a UI corner for rounded edges
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)  -- Rounded corners with radius of 12
corner.Parent = toggleButton

-- Add hover effect
toggleButton.MouseEnter:Connect(function()
    toggleButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)  -- Darker gray on hover for effect
    toggleButton.TextColor3 = Color3.fromRGB(255, 200, 0)  -- Goldish text color on hover for an edgy pop
end)

toggleButton.MouseLeave:Connect(function()
    toggleButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)  -- Return to original dark color
    toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text when not hovered
end)

-- Function to toggle HeadSit
local function toggleHeadSit()
    headSitActive = not headSitActive
    if headSitActive then
        toggleButton.Text = "HeadSit: On"
        -- Create platforms for nearby players
        for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
            if otherPlayer ~= player then
                createPlatformAboveHead(otherPlayer)
            end
        end
    else
        toggleButton.Text = "HeadSit: Off"
        -- Cleanup all platforms
        for userId, platform in pairs(platforms) do
            platform:Destroy()
            platforms[userId] = nil
        end
    end
end

-- Connect button click to toggle function
toggleButton.MouseButton1Click:Connect(toggleHeadSit)

-- Function to reset the GUI on character respawn
local function resetGui()
    -- If the GUI already exists, destroy it before creating a new one
    if player.PlayerGui:FindFirstChild("HeadSitGui") then
        player.PlayerGui.HeadSitGui:Destroy()
    end
    -- Create a new ScreenGui and Button
    screenGui = Instance.new("ScreenGui")
    screenGui.Name = "HeadSitGui"
    screenGui.Parent = player:WaitForChild("PlayerGui")
    screenGui.ResetOnSpawn = false -- Prevent GUI from disappearing on reset

    toggleButton = Instance.new("TextButton", screenGui)
    toggleButton.Name = "HeadSitButton"
    toggleButton.Size = UDim2.new(0, 100, 0, 50)  -- Keep the same size as before
    toggleButton.Position = UDim2.new(0, 10, 1, -150)  -- Position at the top right of the screen
    toggleButton.Text = "HeadSit: Off"
    toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)  -- White text for contrast
    toggleButton.BackgroundColor3 = Color3.fromRGB(35, 35, 35)  -- Dark background for edgy feel
    toggleButton.Font = Enum.Font.GothamBold  -- Bold and modern font
    toggleButton.TextSize = 20  -- Slightly smaller text size for consistency
    toggleButton.TextStrokeTransparency = 0.7  -- Slight text stroke for sharp contrast
    toggleButton.BorderSizePixel = 0  -- No border to keep the sleek look
    toggleButton.BackgroundTransparency = 0.3  -- Slight transparency for depth

    -- Add a UI corner for rounded edges
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)  -- Rounded corners with radius of 12
    corner.Parent = toggleButton

    -- Reconnect the toggle function to the button
    toggleButton.MouseButton1Click:Connect(toggleHeadSit)
end

-- Connect the character added event to recreate the GUI
player.CharacterAdded:Connect(resetGui)

-- Call to create the initial GUI
resetGui()



